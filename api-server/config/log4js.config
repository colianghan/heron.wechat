{
    "appenders": [
        {
            "type": "console"
        },
        {
            "type": "file",
            "filename": "logs/log.log",
            "pattern": "-yyyy-MM-dd",
            "maxLogSize": 20480,
            "backups": 10,
            "alwaysIncludePattern": false,
            "category": "default"
        }
    ],
    "replaceConsole": true,
    "levels": {
        "dateFileLog": "INFO"
    }
}